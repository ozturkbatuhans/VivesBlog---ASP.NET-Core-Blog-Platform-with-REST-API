@model Article

@{
    ViewBag.Title = "Edit Article";
}

<h1>@ViewBag.Title</h1>

<form method="post">
    
    <partial name="CreateEditPartial" model="Model"/>
    
    <button type="submit" class="btn btn-primary">Save</button>
</form>

@section Scripts
{
    <partial name="_ValidationScriptsPartial" />
}
